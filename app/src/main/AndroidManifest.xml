<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="lab.cmego.com.cmegoclientandroid">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:theme="@style/Theme.AppCompat.Light"
        android:supportsRtl="true">
        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".SignUpActivity">
        </activity>
        <activity android:name=".SignInSignUpActivity">
        </activity>
        <activity
            android:name=".InitialActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".ShowMembershipsActivity">
        </activity>
        <activity
            android:name=".ShowMyMembershipsActivity">
        </activity>
        <activity
            android:name=".ShowAllVehiclesActivity">
        </activity>
        <activity android:name=".create.CreateVehicleActivity">
        </activity>
    </application>


</manifest>